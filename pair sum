int pairSum(int *input, int size, int x)
{
    int count=0;
    
  for(int i = 0; i<size; i++){
        int n = x-input[i];
      for(int j=0; j<size; j++){
          if(input[j]== n){
              count++;
          }
          else if(i== j){ continue;}
          
      }
  }
    return count/2;
}
      
      
        
